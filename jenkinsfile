pipeline{
    agent{ label 'SPC_MAVEN' }
    stages{
        stage('vcs'){
            steps{
                git url: 'https://github.com/archanaraj-m/dummyspring.git',
                    branch: 'main'
            }
        }
        stage('package'){
            tools{
                jdk 'JDK_17_UBUNTU'
            }
            steps{
                sh 'mvn -v'
            }
        }
        stage('Build'){
	        steps{
		      sh 'gradle build'
	        }
        }
        
    }
}